name = "a2a-summary"
main = "src/index.ts"
compatibility_date = "2024-01-01"
compatibility_flags = ["nodejs_compat"]

[ai]
binding = "AI"

[[kv_namespaces]]
binding = "SUMMARIES"
id = "PLACEHOLDER_KV_ID"
preview_id = "PLACEHOLDER_PREVIEW_ID"

[[services]]
binding = "MCP_REGISTRY"
service = "a2a-mcp-registry"

[observability]
enabled = true

[[routes]]
pattern = "agent.summary.demos.build"
custom_domain = true

[vars]
ENVIRONMENT = "development"
AUTH_SCHEME = "none"